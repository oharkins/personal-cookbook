  "steps": [
    {
      "description": "Preheat your grill to 400\u00b0F (204\u00b0C). Wash and dry the potatoes thoroughly.",
      "ingredients": [
        {
          "item": "large russet potatoes",
          "amount": 4,
          "unit": "whole"
        }
      ],
      "equipment": [
        {
          "item": "grill"
        }
      ]
    },
    {
      "description": "Rub each potato with olive oil and season generously with BBQ rub. Place potatoes directly on the grill grates.",
      "ingredients": [
        {
          "item": "olive oil",
          "amount": 2,
          "unit": "tablespoon"
        },
        {
          "item": "BBQ rub",
          "amount": 2,
          "unit": "tablespoon"
        }
      ],
      "cookTime": {
        "amount": 60,
        "unit": "minutes"
      }
    },
    {
      "description": "While potatoes are cooking, prepare the toppings. Shred the cheese, chop the green onions, and warm up the pulled pork.",
      "ingredients": [
        {
          "item": "shredded cheddar cheese",
          "amount": 2,
          "unit": "cups"
        },
        {
          "item": "green onions",
          "amount": 4,
          "unit": "whole"
        },
        {
          "item": "pulled pork",
          "amount": 2,
          "unit": "cups"
        }
      ]
    },
    {
      "description": "Once potatoes are tender (about 60 minutes), remove them from the grill. Cut each potato lengthwise and fluff the insides with a fork.",
      "equipment": [
        {
          "item": "fork"
        }
      ]
    },
    {
      "description": "Stuff each potato with pulled pork, drizzle with BBQ sauce, and top with shredded cheese. Return to the grill until cheese is melted.",
      "ingredients": [
        {
          "item": "BBQ sauce",
          "amount": 0.5,
          "unit": "cup"
        }
      ],
      "cookTime": {
        "amount": 5,
        "unit": "minutes"
      }
    },
    {
      "description": "Garnish with chopped green onions and serve immediately.",
      "ingredients": [
        {
          "item": "green onions",
          "amount": 2,
          "unit": "whole"
        }
      ]
    }
  ],
